<ul class="cardsD panel x{{ series.columns }}">
    {%- for article in series.articles %}
    <li>
        <a href="./{{ path }}{{ article.url }}.md">
            <div class="cardSize">
                <div class="cardPadding">
                    <div class="card">
                        <div class="cardImageOuter">
                            <div class="cardImage bgdAccent1 cardScaleImage" style="background-image: url('./{{ path }}images/{{ article.url }}.svg');">
                            </div>
                        </div>
                        <div class="cardText">
                            <h3>{{ article.cardTitle }}</h3>
                        </div>
                    </div>
                </div>
            </div>
        </a>
    </li>
    {%- endfor %}
</ul>